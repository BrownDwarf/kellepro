;--------------------
; OPEN SYMBOLS and POLYGONS
;-------------------

plotsym,0 ;CIRCLE


vsym, 4 ; SQUARE


;MAKE MY OWN FRIGGING TRIANGLE
usersym,$
[0.866025, -4.37114e-08,    -0.866025,     0.866026,-4.37114e-08 ],$
[-0.500000,      1.00000,    -0.500000,    -0.500000,1]

;UPSIDE DOWN TRIANGLE
usersym,$
[0.866025, -4.37114e-08,    -0.866025,     0.866026,-4.37114e-08 ],$
[0.5, -1, 0.5, 0.5, -1]


plotsym,3 ; STAR

vsym, 4, rot=45 ; DIAMOND

vsym, 5 ;pentagon

vsym, 4, /star ;4 pt star

;--------------------
; SKELETONS
;-------------------